
@using DaZhongTransitionLiquidation.Infrastructure.DbEntity
@using DaZhongTransitionLiquidation.Areas.AssetManagement.Models;
@{
    ViewBag.Title = "车龄分析";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var user = ViewBag.User as Sys_User;
    var permission = user.GetCurrentPagePermission();
}
@section js{
    <link href="@Url.CusContent("~/_theme/_js/pivot.css")" rel="stylesheet" />
    <script src="@Url.CusContent("~/Areas/AnalysisManagementCenter/Views/_js/VehicleAgeReport.js")"></script>
    <script src="@Url.CusContent("/_theme/DatePicker/WdatePicker.js")"></script>
    <script src="~/_theme/_js/jquery-ui-1.8.20.js"></script>
    <script src="~/_theme/_js/jquery-ui-1.8.20.min.js"></script>
    <script src="~/_theme/_js/pivot.js"></script>
}
<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">查询</span></div>
    </div>
    <div class="body_mk" style="height: 60px;">
        <table style="margin-top: 20px; width: 400px; margin-left: 20px;">
            <tr style="height: 45px;">
                <td style="width: 50px;text-align: right"><span class="spanText">年份：</span></td>
                <td style="width: 100px;text-align: center">
                    <input id="DateOfYear" type="text" onfocus="WdatePicker({ realDateFmt: 'yyyy', dateFmt: 'yyyy', lang: 'zh-cn'})" class="input_text form-control" style="width: 200px" validatetype="required">
                </td>
                <td style="width: 95px;">
                    <button id="btnSearch" type="button" class="buttons"><i class="iconfont btn_icon">&#xe679;</i><span style="margin-left: 7px; float: left;">查询</span></button>
                </td>
            </tr>
            <tr>
        </table>
    </div>
</div>
<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">数据列表</span></div>
    </div>
    <div id="assetReport" style="width:100%;overflow-x: scroll;display:none">
        <div id='tabs'>
            <ul>
                <li style="margin-left: 30px;">管理公司</li>
                <li>所属公司</li>
            </ul>
            <div id="jqxManageCompanyPeriodTable" class="jqxTable" style="height:500px;background-color: white; float: left;">
            </div>
            <div id="jqxBelongToCompanyPeriodTable" class="jqxTable" style="height:500px;background-color: white; float: left;">
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="EditPermission" value="@permission.Edit.ToString()" />
<input type="hidden" id="NewPermission" value="@permission.New.ToString()" />