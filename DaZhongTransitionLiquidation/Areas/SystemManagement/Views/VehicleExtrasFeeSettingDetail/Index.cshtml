@using DaZhongTransitionLiquidation.Infrastructure.DbEntity
@using DaZhongTransitionLiquidation.Areas.AssetManagement.Models;
@{
    ViewBag.Title = "采购资产订单配置/车辆类资产费用标准明细";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var currentModuleRolePermission = ViewBag.CurrentModulePermission as Sys_Role_Module;
}
@section js{
    <script src="@Url.CusContent("~/Areas/Systemmanagement/Views/_js/VehicleExtrasFeeSettingDetail.js")"></script>
}
<style>
    .input_text {
        width: 150px;
        height: 35px;
        border: 1px;
        border-style: solid;
        border-color: #dddddd;
        -moz-box-shadow: 0px 1px 10px #dddddd;
        -webkit-box-shadow: 0px 1px 10px #dddddd;
        box-shadow: 0 0 10px #dddddd;
        background-color: white !important;
        border-radius: 4px;
        float: left;
    }

    .jqx-combobox-input {
        padding-left: 8px;
        color: #555;
    }

    .jqx-dropdownlist-content {
        margin-left: 5px;
    }

    .jqx-tabs-content-element {
        overflow: auto !important;
    }

    .jqx-cell {
        padding: 0px;
    }
</style>
<table id="hideButton" style="margin-top: 10px;display:block">
    <tr style="height: 50px;">
        <td style="width: 95px;">
            <button id="btnSave" type="button" class="buttons" style=""><i class="iconfont btn_icon" style="color: black !important;">&#xe6f3;</i><span style="margin-left: 7px; float: left;">保存</span></button>
        </td>
        <td style="width: 95px;">
            <button id="btnCancel" type="button" class="buttons" style=""><i class="iconfont btn_icon" style="color: black !important;">&#xe678; </i><span style="margin-left: 7px; float: left;">取消</span></button>
        </td>
    </tr>
</table>

<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">主信息</span></div>
    </div>
    <div class="body_mk" style="height: 440px;">
        <table id="VoucherList" style="width:90%;margin-left:50px;margin-top:20px">
            <tr style="height:50px">
                <td style="text-align: right;">车型：</td>
                <td style="text-align: left;">
                    <select id="VehicleModel" class="input_text form-control" style="width: 200px;">
                    </select>
                </td>
                <td style="text-align: right;">费用：</td>
                <td style="text-align: left;">
                    <input id="Fee" type="text" value="" style="width: 200px;" class="input_text form-control" validatetype="required"/>
                </td>
            </tr>
            <tr style="height:50px">
                <td style="text-align: right;">业务项目：</td>
                <td style="text-align: left;">
                    <div id="BusinessProject"></div>
                </td>
                <td style="text-align: right;">业务编码：</td>
                <td style="text-align: left;" colspan="3">
                    <span id="BusinessSubItem" style="padding-top: 3px;"></span>
                </td>
            </tr>
            <tr style="height:50px">
                <td style="text-align: right;">是否启用：</td>
                <td style="text-align: left;">
                    <select id="Status" class="input_text form-control" style="width: 200px;">
                        <option selected value="1">是</option>
                        <option value="0">否</option>
                    </select>
                </td>
            </tr>
        </table>
        <div style="display: none">
            <input id="VGUID" />
        </div>
    </div>
</div>