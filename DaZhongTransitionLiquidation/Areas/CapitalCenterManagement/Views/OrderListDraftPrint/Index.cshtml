@using DaZhongTransitionLiquidation.Infrastructure.DbEntity
@{
    ViewBag.Title = "支付订单/订单打印";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section js{
    <script src="@Url.Content("~/_theme/DatePicker/WdatePicker.js")"></script>
    <script src="@Url.Content("~/Areas/CapitalCenterManagement/Views/_js/OrderListDraftPrint.js")"></script>
}

<table id="hideButton" style="margin-top: 10px;">
    <tr style="height: 50px;">
        <td style="width: 95px;">
            <button id="btnPrint" type="button" class="buttons" style=""><i class="iconfont btn_icon" style="color: black !important;">&#xe6f3;</i><span style="margin-left: 7px; float: left;">打印</span></button>
        </td>
    </tr>
</table>

<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">打印单据</span></div>
    </div>
    <div class="body_mk" style="height: 1000px;">
        <table style="width:97%">
            <tr style="height:50px">
                <td style="text-align: center;font-size: 40px;" colspan="2">大众交通(集团)股份有限公司大众出租汽车分公司</td>
            </tr>
            <tr style="height:50px">
                <td style="text-align: center;font-size: 40px;" colspan="2">付 款 凭 单</td>
            </tr>
            <tr style="height:30px">
                <td style="text-align: right;" colspan="2">附单据：<label id="AttachmentNumber" for=""></label>张</td>
            </tr>
            <tr style="height:30px">
                <td style="text-align: center;"><label id="SubmitDate" for=""></label></td>
                <td style="text-align: right;">其中发票：<label id="InvoiceNumber" for=""></label>张</td>
            </tr>
        </table>
        <br/>
        <table id="VoucherList" style="width:100%;" border="1" cellspacing="0">
            <tr style="height:30px">
                <td style="text-align: center;font-size: 20px;" rowspan="3" colspan="2">受款人/部门/单位</td>

                <td style="text-align: center;width: 1000px;" rowspan="3">123</td>
                <td style="text-align: center;font-size: 20px;" colspan="3">支付方式</td>
            </tr>
            <tr style="height:30px">
                <td style="text-align: center;width: 100px;">现金</td>
                <td style="text-align: center;width: 100px;">银行</td>
                <td style="text-align: center;width: 100px;">其他</td>
            </tr>
            <tr style="height:30px">

                <td style="text-align: center;"></td>
                <td style="text-align: center;"></td>
                <td style="text-align: center;"></td>
            </tr>
            <tr style="height:400px">
                <td style="text-align: center;width: 100px;font-size: 20px;" >付  款<br>内  容</td>
                <td style="text-align: center;" colspan="5">12345567899</td>
            </tr>
            <tr style="height:90px">
                <td style="text-align: center;font-size: 20px;">金额</td>
                <td style="text-align: left;font-size: 20px;" colspan="2">&nbsp;&nbsp;人民币<br>（大写）</td>

                <td style="text-align: left;font-size: 25px;" colspan="3">&nbsp;&nbsp;¥ </td>
            </tr>
        </table>
        <br/>
        <table style="width: 100%;">
            <tr style="height:80px">
                <td style="text-align: left;width: 50px;">企 负<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;责<br>业 人</td>
                <td style="text-align: left;"><label id="EnterpriseLeader" for="">1</label></td>
                <td style="text-align: left;width: 50px;">分 负<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;责<br>管 人</td>
                <td style="text-align: left;"><label id="ResponsibleLeader" for="">2</label></td>
                <td style="text-align: left;width: 50px;">计 审<br>财<br>部 核</td>
                <td style="text-align: left;"><label id="JiCaiBuExamine" for="">3</label></td>
                <td style="text-align: left;width: 50px;">部 主<br><br>门 管</td>
                <td style="text-align: left;"><label id="DepartmentHead" for="">4</label></td>
                <td style="text-align: left;width: 50px;">出<br><br>纳</td>
                <td style="text-align: left;"><label id="Cashier" for="">5</label></td>
                <td style="text-align: left;width: 50px;">受 经<br>款 办<br>人 人</td>
                <td style="text-align: left;"><label id="Payee" for="">6</label></td>
            </tr>
        </table>
    </div>
</div>