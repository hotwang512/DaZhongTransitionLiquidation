@using DaZhongTransitionLiquidation.Infrastructure.DbEntity
@using DaZhongTransitionLiquidation.Areas.AssetManagement.Models;
@{
    ViewBag.Title = "固定资产净值";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var user = ViewBag.User as Sys_User;
    var permission = user.GetCurrentPagePermission();
}
@section js{
    <script src="@Url.CusContent("~/Areas/AnalysisManagementCenter/Views/_js/FixedAssetsNetValue.js")"></script>
    <script src="@Url.CusContent("/_theme/DatePicker/WdatePicker.js")"></script>
}
<table id="hideButton" style="margin-top: 10px;display:block">
    <tr style="height: 50px;">
        @if (permission.New || permission.Edit)
        {
            <td style="width: 95px;">
                <button id="btnSave" type="button" class="buttons" style=""><i class="iconfont btn_icon" style="color: black !important;">&#xe634;</i><span style="margin-left: 7px; float: left;">保存</span></button>
            </td>
        }
    </tr>
</table>
<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">查询</span></div>
    </div>
    <div class="body_mk" style="height: 60px;">
        <table style="margin-top: 20px; width: 1000px; margin-left: 20px;">
            <tr style="height: 45px;">
                <td style="width: 50px;text-align: right"><span class="spanText">年份：</span></td>
                <td style="width: 100px;text-align: center">
                    <input id="DateOfYear" type="text" onfocus="WdatePicker({ realDateFmt: 'yyyy', dateFmt: 'yyyy', lang: 'zh-cn', onpicked: pickedFunc })" class="input_text form-control" style="width: 200px" validatetype="required">
                </td>
                <td style="width: 50px;text-align: right"><span class="spanText">月份：</span></td>
                <td style="width: 400px; text-align: center;padding-top:5px">
                    <label><input name="Month" type="checkbox" value="1" />1月</label>
                    <label><input name="Month" type="checkbox" value="2" />2月</label>
                    <label><input name="Month" type="checkbox" value="3" />3月</label>
                    <label><input name="Month" type="checkbox" value="4" />4月</label>
                    <label><input name="Month" type="checkbox" value="5" />5月</label>
                    <label><input name="Month" type="checkbox" value="6" />6月</label>
                    <label><input name="Month" type="checkbox" value="7" />7月</label>
                    <label><input name="Month" type="checkbox" value="8" />8月</label>
                    <label><input name="Month" type="checkbox" value="9" />9月</label>
                    <label><input name="Month" type="checkbox" value="10" />10月</label>
                    <label><input name="Month" type="checkbox" value="11" />11月</label>
                    <label><input name="Month" type="checkbox" value="12" />12月</label>
                </td>
                <td style="width: 95px;">
                    <button id="btnSearch" type="button" class="buttons"><i class="iconfont btn_icon">&#xe679;</i><span style="margin-left: 7px; float: left;">查询</span></button>
                </td>
            </tr>
            <tr>
        </table>
    </div>
</div>
<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">数据列表</span></div>
    </div>
    <div class="body_mk" style="height: 320px;">
        <div id="table"></div>
        <div style="display: none">
            <input id="VGUID" />
        </div>
    </div>
</div>
<input type="hidden" id="EditPermission" value="@permission.Edit.ToString()" />
<input type="hidden" id="NewPermission" value="@permission.New.ToString()" />