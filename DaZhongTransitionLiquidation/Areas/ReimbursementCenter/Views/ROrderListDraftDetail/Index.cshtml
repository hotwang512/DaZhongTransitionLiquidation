        overflow: auto !important;
    }

    #btn_AttachmentBrowseButton {
        width: auto;
        min-width: 277px;
        min-width: 570px;
        height: 40px;
        margin: 0px 10px 10px;
        cursor: pointer;
        padding: 0px 20px;
        border: 0px solid rgb(72, 184, 232);
        color: rgb(204, 204, 204);
        outline: none;
        font-size: 15px;
        font-weight: bold;
        position: initial !important;
        background: rgb(0, 153, 204) !important;
    }

