@using DaZhongTransitionLiquidation.Infrastructure.DbEntity
@{
    ViewBag.Title = "结算标准/车辆经营模式天数";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section js{
    <script src="@Url.CusContent("~/_theme/DatePicker/WdatePicker.js")"></script>
    <script src="@Url.CusContent("~/Areas/VoucherManageManagement/Views/_js/VehicleBusiness.js")"></script>
    <script src="~/_theme/_js/jquery-ui-1.8.20.js"></script>
    <script src="~/_theme/_js/jquery-ui-1.8.20.min.js"></script>
    <script src="~/_theme/_js/pivot.js"></script>
}

@section css{
    <link href="@Url.CusContent("~/_theme/_js/pivot.css")" rel="stylesheet" />
}
<style type="text/css">
    .jqx-grid-cell-pinned-office {
        background-color: none !important;
    }
</style>

<table style="margin-top: 10px;">
    <tr style="height: 50px;">
        <td style="width: 190px;">
            <button id="btnSync" type="button" style="width: 180px;" class="buttons"><i class="iconfont btn_icon" style="color: black !important;">&#xebda;</i><span style="margin-left: 7px; float: left;">同步上月车辆数据</span></button>
        </td>
    </tr>
</table>



<!-- 中部条件 -->
<div class="mk" style="">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">查询</span></div>
    </div>
    <div class="body_mk" style="height: 90px;">
            <table style="margin-top: 20px; margin-left: 20px;">
                <tr style="height: 45px;">
                    <td class="td_span">日期：</td>
                    <td class="td_input">
                        <input id="YearMonth" type="text" style="width: 210px;" class="input_text form-control" onfocus="WdatePicker({ maxDate: '#F{$dp.$D(\'YearMonth\')||\'%y-#{%M}\'}', realDateFmt: 'yyyy-MM', dateFmt: 'yyyy-MM', lang: 'zh-cn' })" />
                    </td>
                    <td style="width: 95px;">
                        <button id="btnSearch" type="button" class="buttons"><i class="iconfont btn_icon">&#xe679;</i><span style="margin-left: 7px; float: left;">查询</span></button>
                    </td>
                </tr>
            </table>
    </div>
</div>
<!-- 中部条件 -->
<!-- 底部表格 -->
<div class="mk">
    <div class="header_mk">
        <div class="header_icon"></div>
        <div class="header_text_div"><span class="header_text_span">数据列表</span></div>
    </div>
    <div id="jqxTable" class="jqxTable" style="display:none">
    </div>
</div>
<style>
    /*.input_text {
        width: 340px;
    }*/
    .td_span {
        text-align: right;
        width: 115px;
    }
</style>
