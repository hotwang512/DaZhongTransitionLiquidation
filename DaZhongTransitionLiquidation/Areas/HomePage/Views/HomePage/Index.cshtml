        <div class="home-message" style="text-align: left;">
            @{
                var moduleMenu = user.Permission;
                foreach (var item in moduleMenu)
                {
                    if (item.Parent == null)
                    {
                        <a url="" href="@item.Url" id="" class="moudle" data-coordinate="1">
                            <div class="home-message-modular">
                                <div class="img_Parent_div">
                                    <img class="img-style" src="@item.Icon" />
                                </div>
                                <span class="writing">@item.Name</span>
